<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingcheng.marketing.business.mapper.CrmSchoolMapper">

    <select id="queryPage" parameterType="java.util.Map" resultType="java.util.Map">
        select
        *
        from crm_school
        <where>
            1 = 1
             <if test="obj.id != null and obj.id != ''">
                 and id = #{obj.id}
             </if>
             <if test="obj.province != null and obj.province != ''">
                 and province = #{obj.province}
             </if>
             <if test="obj.city != null and obj.city != ''">
                 and city = #{obj.city}
             </if>
             <if test="obj.region != null and obj.region != ''">
                 and region = #{obj.region}
             </if>
             <if test="obj.street != null and obj.street != ''">
                 and street = #{obj.street}
             </if>
             <if test="obj.school != null and obj.school != ''">
                 and school = #{obj.school}
             </if>
             <if test="obj.grade != null and obj.grade != ''">
                 and grade = #{obj.grade}
             </if>
             <if test="obj.address != null and obj.address != ''">
                 and address = #{obj.address}
             </if>
             <if test="obj.addressCode != null and obj.addressCode != ''">
                 and address_code = #{obj.addressCode}
             </if>
             <if test="obj.lat != null">
                 and lat = #{obj.lat}
             </if>
             <if test="obj.lng != null">
                 and lng = #{obj.lng}
             </if>
             <if test="obj.isDeleted != null">
                 and is_deleted = #{obj.isDeleted}
             </if>
             <if test="obj.versionNum != null and obj.versionNum != ''">
                 and version_num = #{obj.versionNum}
             </if>
             <if test="obj.ctime != null and obj.ctime != ''">
                 and ctime = #{obj.ctime}
             </if>
             <if test="obj.createdBy != null and obj.createdBy != ''">
                 and created_by = #{obj.createdBy}
             </if>
             <if test="obj.utime != null and obj.utime != ''">
                 and utime = #{obj.utime}
             </if>
             <if test="obj.lastUpdatedBy != null and obj.lastUpdatedBy != ''">
                 and last_updated_by = #{obj.lastUpdatedBy}
             </if>
        </where>
    </select>
</mapper>