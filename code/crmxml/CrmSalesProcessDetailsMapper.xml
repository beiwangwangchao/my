<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingcheng.marketing.business.mapper.CrmSalesProcessDetailsMapper">

    <select id="queryPage" parameterType="java.util.Map" resultType="java.util.Map">
        select
        *
        from crm_sales_process_details
        <where>
            1 = 1
             <if test="obj.id != null and obj.id != ''">
                 and id = #{obj.id}
             </if>
             <if test="obj.sourceId != null and obj.sourceId != ''">
                 and source_id = #{obj.sourceId}
             </if>
             <if test="obj.type != null and obj.type != ''">
                 and type = #{obj.type}
             </if>
             <if test="obj.productId != null and obj.productId != ''">
                 and product_id = #{obj.productId}
             </if>
             <if test="obj.contactType != null and obj.contactType != ''">
                 and contact_type = #{obj.contactType}
             </if>
             <if test="obj.status != null and obj.status != ''">
                 and status = #{obj.status}
             </if>
             <if test="obj.nextFollowTime != null and obj.nextFollowTime != ''">
                 and next_follow_time = #{obj.nextFollowTime}
             </if>
             <if test="obj.remark != null and obj.remark != ''">
                 and remark = #{obj.remark}
             </if>
             <if test="obj.picture != null and obj.picture != ''">
                 and picture = #{obj.picture}
             </if>
             <if test="obj.isDeleted != null">
                 and is_deleted = #{obj.isDeleted}
             </if>
             <if test="obj.versionNum != null">
                 and version_num = #{obj.versionNum}
             </if>
             <if test="obj.ctime != null and obj.ctime != ''">
                 and ctime = #{obj.ctime}
             </if>
             <if test="obj.createdBy != null and obj.createdBy != ''">
                 and created_by = #{obj.createdBy}
             </if>
             <if test="obj.utime != null and obj.utime != ''">
                 and utime = #{obj.utime}
             </if>
             <if test="obj.lastUpdatedBy != null and obj.lastUpdatedBy != ''">
                 and last_updated_by = #{obj.lastUpdatedBy}
             </if>
             <if test="obj.fromId != null and obj.fromId != ''">
                 and from_id = #{obj.fromId}
             </if>
             <if test="obj.toId != null and obj.toId != ''">
                 and to_id = #{obj.toId}
             </if>
             <if test="obj.fromStatus != null and obj.fromStatus != ''">
                 and from_status = #{obj.fromStatus}
             </if>
             <if test="obj.toStatus != null and obj.toStatus != ''">
                 and to_status = #{obj.toStatus}
             </if>
             <if test="obj.transactionType != null and obj.transactionType != ''">
                 and transaction_type = #{obj.transactionType}
             </if>
             <if test="obj.salesDate != null and obj.salesDate != ''">
                 and sales_date = #{obj.salesDate}
             </if>
             <if test="obj.salesAmount != null">
                 and sales_amount = #{obj.salesAmount}
             </if>
        </where>
    </select>
</mapper>