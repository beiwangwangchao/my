<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingcheng.marketing.business.mapper.CrmTrialClazzMapper">

    <select id="queryPage" parameterType="java.util.Map" resultType="java.util.Map">
        select
        *
        from crm_trial_clazz
        <where>
            1 = 1
             <if test="obj.id != null and obj.id != ''">
                 and id = #{obj.id}
             </if>
             <if test="obj.orgId != null and obj.orgId != ''">
                 and org_id = #{obj.orgId}
             </if>
             <if test="obj.organizationId != null and obj.organizationId != ''">
                 and organization_id = #{obj.organizationId}
             </if>
             <if test="obj.classroomId != null and obj.classroomId != ''">
                 and classroom_id = #{obj.classroomId}
             </if>
             <if test="obj.classId != null and obj.classId != ''">
                 and class_id = #{obj.classId}
             </if>
             <if test="obj.type != null and obj.type != ''">
                 and type = #{obj.type}
             </if>
             <if test="obj.teacherId != null and obj.teacherId != ''">
                 and teacher_id = #{obj.teacherId}
             </if>
             <if test="obj.classDate != null and obj.classDate != ''">
                 and class_date = #{obj.classDate}
             </if>
             <if test="obj.beginTime != null and obj.beginTime != ''">
                 and begin_time = #{obj.beginTime}
             </if>
             <if test="obj.endTime != null and obj.endTime != ''">
                 and end_time = #{obj.endTime}
             </if>
             <if test="obj.num != null">
                 and num = #{obj.num}
             </if>
             <if test="obj.status != null">
                 and status = #{obj.status}
             </if>
             <if test="obj.explains != null and obj.explains != ''">
                 and explains = #{obj.explains}
             </if>
             <if test="obj.ctime != null and obj.ctime != ''">
                 and ctime = #{obj.ctime}
             </if>
             <if test="obj.utime != null and obj.utime != ''">
                 and utime = #{obj.utime}
             </if>
             <if test="obj.createdBy != null and obj.createdBy != ''">
                 and created_by = #{obj.createdBy}
             </if>
             <if test="obj.lastUpdatedBy != null and obj.lastUpdatedBy != ''">
                 and last_updated_by = #{obj.lastUpdatedBy}
             </if>
             <if test="obj.versionNum != null">
                 and version_num = #{obj.versionNum}
             </if>
             <if test="obj.isPut != null">
                 and is_put = #{obj.isPut}
             </if>
             <if test="obj.isDeleted != null">
                 and is_deleted = #{obj.isDeleted}
             </if>
             <if test="obj.week != null">
                 and week = #{obj.week}
             </if>
             <if test="obj.perId != null and obj.perId != ''">
                 and per_id = #{obj.perId}
             </if>
             <if test="obj.finalOpenGradeTime != null and obj.finalOpenGradeTime != ''">
                 and final_open_grade_time = #{obj.finalOpenGradeTime}
             </if>
             <if test="obj.claim != null and obj.claim != ''">
                 and claim = #{obj.claim}
             </if>
        </where>
    </select>
</mapper>