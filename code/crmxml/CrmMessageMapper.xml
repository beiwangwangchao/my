<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingcheng.marketing.business.mapper.CrmMessageMapper">

    <select id="queryPage" parameterType="java.util.Map" resultType="java.util.Map">
        select
        *
        from crm_message
        <where>
            1 = 1
             <if test="obj.id != null and obj.id != ''">
                 and id = #{obj.id}
             </if>
             <if test="obj.content != null and obj.content != ''">
                 and content = #{obj.content}
             </if>
             <if test="obj.status != null and obj.status != ''">
                 and status = #{obj.status}
             </if>
             <if test="obj.fromUserId != null and obj.fromUserId != ''">
                 and from_user_id = #{obj.fromUserId}
             </if>
             <if test="obj.toUserId != null and obj.toUserId != ''">
                 and to_user_id = #{obj.toUserId}
             </if>
             <if test="obj.sendTime != null and obj.sendTime != ''">
                 and send_time = #{obj.sendTime}
             </if>
             <if test="obj.isDeleted != null">
                 and is_deleted = #{obj.isDeleted}
             </if>
             <if test="obj.versionNum != null">
                 and version_num = #{obj.versionNum}
             </if>
             <if test="obj.ctime != null and obj.ctime != ''">
                 and ctime = #{obj.ctime}
             </if>
             <if test="obj.createdBy != null and obj.createdBy != ''">
                 and created_by = #{obj.createdBy}
             </if>
             <if test="obj.utime != null and obj.utime != ''">
                 and utime = #{obj.utime}
             </if>
             <if test="obj.lastUpdatedBy != null and obj.lastUpdatedBy != ''">
                 and last_updated_by = #{obj.lastUpdatedBy}
             </if>
        </where>
    </select>
</mapper>