<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingcheng.marketing.business.mapper.CrmMenuMapper">

    <select id="queryPage" parameterType="java.util.Map" resultType="java.util.Map">
        select
        *
        from crm_menu
        <where>
            1 = 1
             <if test="obj.id != null and obj.id != ''">
                 and id = #{obj.id}
             </if>
             <if test="obj.ctime != null and obj.ctime != ''">
                 and ctime = #{obj.ctime}
             </if>
             <if test="obj.createdBy != null and obj.createdBy != ''">
                 and created_by = #{obj.createdBy}
             </if>
             <if test="obj.utime != null and obj.utime != ''">
                 and utime = #{obj.utime}
             </if>
             <if test="obj.lastUpdatedBy != null and obj.lastUpdatedBy != ''">
                 and last_updated_by = #{obj.lastUpdatedBy}
             </if>
             <if test="obj.versionNum != null">
                 and version_num = #{obj.versionNum}
             </if>
             <if test="obj.name != null and obj.name != ''">
                 and name = #{obj.name}
             </if>
             <if test="obj.code != null and obj.code != ''">
                 and code = #{obj.code}
             </if>
             <if test="obj.parentId != null and obj.parentId != ''">
                 and parent_id = #{obj.parentId}
             </if>
             <if test="obj.sort != null">
                 and sort = #{obj.sort}
             </if>
             <if test="obj.logo != null and obj.logo != ''">
                 and logo = #{obj.logo}
             </if>
             <if test="obj.isFreeze != null">
                 and is_freeze = #{obj.isFreeze}
             </if>
             <if test="obj.isHide != null">
                 and is_hide = #{obj.isHide}
             </if>
             <if test="obj.isDeleted != null">
                 and is_deleted = #{obj.isDeleted}
             </if>
             <if test="obj.isApp != null">
                 and is_app = #{obj.isApp}
             </if>
        </where>
    </select>
</mapper>